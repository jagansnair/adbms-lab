mysql> select avg(dept_avg_salary) as overall_avg_salary from (select department_id,avg(salary) as dept_avg_salary from employees group by department_id) as dept_avg_salary_subquery; 
+--------------------+
| overall_avg_salary |
+--------------------+
|     10780.00000000 |
+--------------------+
1 row in set (0.00 sec)

    
SELECT employee_id,first_name,last_name, salary,(SELECT ROUND(AVG(salary), 0) FROM employees) AS average_salary,salary - (SELECT ROUND(AVG(salary), 0) FROM employees) AS difference FROM employees ORDER BY first_name, last_name;
    
