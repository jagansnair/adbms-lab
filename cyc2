SELECT employee_id, first_name, last_name, salary FROM employees WHERE salary = (SELECT MAX(salary) FROM employees) ORDER BY first_name , last_name;
+-------------+------------+-----------+--------+
| employee_id | first_name | last_name | salary |
+-------------+------------+-----------+--------+
|         100 | Steven     | King      |  24000 |
+-------------+------------+-----------+--------+
1 row in set (0.00 sec)


SELECT employee_id, first_name, last_name, salary FROM employees WHERE salary > (SELECT AVG(salary) FROM employees) 

+-------------+------------+-----------+--------+
| employee_id | first_name | last_name | salary |
+-------------+------------+-----------+--------+
|         100 | Steven     | King      |  24000 |
|         101 | Neena      | Kochhar   |  17000 |
|         102 | Lex        | De Haan   |  17000 |
|         108 | Nancy      | Greenberg |  12000 |
+-------------+------------+-----------+--------+

SELECT department_id,department_name FROM departments d WHERE EXISTS( SELECT 1 FROM employees e WHERE salary > 10000 AND e.department_id = d.department_id) ORDER BY department_name;
+---------------+-----------------+
| department_id | department_name |
+---------------+-----------------+
|          1000 | Administration  |
|          1800 | Executive       |
|          1100 | Marketing       |
|          1200 | Purchasing      |
+---------------+-----------------+

SELECT department_id,department_name FROM departments d WHERE EXISTS( SELECT 1 FROM employees e WHERE salary < 10000 AND e.department_id = d.department_id) ORDER BY department_name;
+---------------+------------------+
| department_id | department_name  |
